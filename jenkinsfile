pipeline {
    agent any

    stages {
                    stage('load parameters') { 
            steps {
                load "parameters.groovy"
            }
        }
        stage('Build') {
            steps {
            if (env.SAST_GIT_URL == 'null') {
                echo " sast value ${env.SAST_GIT_URL}............................ "
           } else {
               echo " sast value ${env.SAST_GIT_URL}............................ "
        }
        }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo " sast value ${env.SAST_GIT_URL}............................ "

            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
