pipeline {
    agent any

    stages {
                    stage('load parameters') { 
            steps {
                load "parameters.groovy"
            }
        }
        stage('Build') {
                    when {
                    expression {
                    env.SAST_GIT_URL.isEmpty() == null
                    }
                    }
            steps {
                echo 'Building..'
                echo " sast value ${env.SAST_GIT_URL}............................ "
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
                echo " sast value ${env.SAST_GIT_URL}............................ "

            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
